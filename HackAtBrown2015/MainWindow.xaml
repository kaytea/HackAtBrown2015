<Window x:Class="HackAtBrown2015.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Image Name="camera"/>
        <Grid>
            <StackPanel>
                <Button Content="Color" Click="Color_Click"></Button>
                <Button Content="Depth" Click="Depth_Click"></Button>
                <Button Content="Infrared" Click="Infrared_Click"></Button>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
